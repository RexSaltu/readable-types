{
  "name": "readable-types",
  "description": "Utils for typescript language",
  "author": "eythaann",
  "version": "0.9.5",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Eythaann/readable-types"
  },
  "bugs": {
    "url": "https://github.com/Eythaann/readable-types/issues"
  },
